	<bean name="order" class="com.sai.spring.setterInjecton.Order">

		<property name="productName">
			<value>Apple Macbook</value>
		</property>

		<property name="productId">
			<value>1002</value>
		</property>

		<property name="orderDate">
			<value>10/02/2003</value>
		</property>

		<property name="customer">
			<ref bean="cus3" />
		</property>
	</bean>

	<!-- Constructor injection -->

	<bean name="customer"
		class="com.sai.spring.setterInjecton.Customer">
		<constructor-arg value="Sai Srinivas22"
			type="java.lang.String" />
		<constructor-arg>
			<value>9502168833</value>
		</constructor-arg>
		<constructor-arg>
			<value>Hyderabad</value>
		</constructor-arg>
	</bean>

	<bean name="ord" class="com.sai.spring.setterInjecton.Order">
		<constructor-arg type="int">
			<value>86868</value>
		</constructor-arg>
		<constructor-arg>
			<value>One plus mobile</value>
		</constructor-arg>
		<constructor-arg>
			<value>22/09/2022</value>
		</constructor-arg>
		<constructor-arg>
			<ref bean="cus2" />
		</constructor-arg>
	</bean>


	<bean name="ord1" class="com.sai.spring.setterInjecton.Order">
		<constructor-arg type="int" index="1">
			<value>86868</value>
		</constructor-arg>
		<constructor-arg index="0">
			<value>One plus mobile</value>
		</constructor-arg>
	</bean>




</beans>